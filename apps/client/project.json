{
  "name": "client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "techbir",
  "sourceRoot": "apps/client/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/client",
        "index": "apps/client/src/index.html",
        "main": "apps/client/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/client/tsconfig.app.json",
        "assets": [
          "apps/client/src/favicon.ico",
          "apps/client/src/assets",
          "apps/client/src/manifest.webmanifest"
        ],
        "styles": [
          "apps/client/src/styles.scss"
        ],
        "scripts": [],
        "serviceWorker": true,
        "ngswConfigPath": "apps/client/ngsw-config.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "client:build:production"
        },
        "development": {
          "browserTarget": "client:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "client:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/client/**/*.ts",
          "apps/client/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/client/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "client:build",
        "outputPath": "apps/webapp/src/locale"
      }
    }
  },
  "i18n": {
    "locales": {
      "": {
        "name": "Default",
        "translation": "apps/client/src/locale/messages.xlf"
      },
      "en": {
        "name": "English (United States)",
        "translation": "apps/client/src/locale/messages.en_US.xlf"
      },
      "es": {
        "name": "Spanish (Spain)",
        "translation": "apps/client/src/locale/messages.es_ES.xlf"
      },
      "zh": {
        "name": "Mandarin Chinese (Simplified)",
        "translation": "apps/client/src/locale/messages.zh_CN.xlf"
      },
      "hi": {
        "name": "Hindi (India)",
        "translation": "apps/client/src/locale/messages.hi_IN.xlf"
      },
      "ar": {
        "name": "Arabic (Egypt)",
        "translation": "apps/client/src/locale/messages.ar_EG.xlf"
      },
      "fr": {
        "name": "French (France)",
        "translation": "apps/client/src/locale/messages.fr_FR.xlf"
      },
      "ru": {
        "name": "Russian (Russia)",
        "translation": "apps/client/src/locale/messages.ru_RU.xlf"
      },
      "pt": {
        "name": "Portuguese (Portugal)",
        "translation": "apps/client/src/locale/messages.pt_PT.xlf"
      },
      "bn": {
        "name": "Bengali (India)",
        "translation": "apps/client/src/locale/messages.bn_IN.xlf"
      },
      "ur": {
        "name": "Urdu (Pakistan)",
